{
    "name": "@hubblecommerce/hubble",
    "version": "1.1.7",
    "description": "PWA for eCommerce frameworks based on NuxtJs",
    "author": "hubble commerce",
    "license": "MIT",
    "main": "module.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "postinstall": "node ./scripts/post-install.js",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
        "prettier": "prettier --check '**/*.{scss,js,vue}'",
        "prettier:fix": "prettier --write '**/*.{scss,js,vue}'",
        "test:hubble": "jest --testPathPattern=/__tests__/ --config=modules/hubble-frontend-pwa/@hubblecommerce/hubble/jest.config.js",
        "test:components": "jest --testPathPattern=/components/ --config=modules/hubble-frontend-pwa/@hubblecommerce/hubble/jest.config.js",
        "test:store": "jest --testPathPattern=/store/ --config=modules/hubble-frontend-pwa/@hubblecommerce/hubble/jest.config.js"
    },
    "dependencies": {
        "@hubblecommerce/amazon-pay": "^1.0.0",
        "@hubblecommerce/payone": "^1.0.0",
        "@nuxtjs/axios": "^5.12.0",
        "@nuxtjs/recaptcha": "^0.6.1",
        "@nuxtjs/pwa": "^3.0.0-beta.19",
        "@nuxtjs/dotenv": "^1.4.1",
        "@nuxtjs/google-analytics": "^2.2.1",
        "@nuxtjs/google-tag-manager": "^2.3.1",
        "nuxt-mq": "^1.1.3",
        "nuxt-i18n": "^6.4.0",
        "cookie-universal-nuxt": "^2.0.18",
        "localforage-nuxt": "1.0.5",
        "chokidar": "^3.4.2",
        "cross-env": "^5.2.1",
        "defu": "^3.1.0",
        "document-offset": "^1.0.4",
        "fs-extra": "^9.0.1",
        "globby": "^11.0.1",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "v-click-outside": "^2.1.5",
        "vee-validate": "^3.0.11",
        "vue-check-view": "^0.3.0",
        "vue-js-modal": "^1.3.34",
        "vue-slider-component": "^2.8.2",
        "vue-tiny-slider": "0.1.35",
        "winston": "^3.2.1",
        "@babel/cli": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "babel-plugin-lodash": "^3.3.4",
        "base-64": "^0.1.0",
        "cypress": "^4.4.1",
        "faker": "^4.1.0",
        "lozad": "^1.14.0",
        "node-sass": "^4.13.0",
        "nodemon": "^1.19.4",
        "sass-loader": "^7.3.1",
        "nuxt-purgecss": "^1.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@vue/test-utils": "^1.1.2",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^26.6.3",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-vue": "^7.4.0",
        "husky": "^4.3.6",
        "jest": "^26.6.3",
        "jest-expect-message": "^1.0.2",
        "jest-serializer-vue": "^2.0.2",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "vue-jest": "^3.0.7"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hubblecommerce/hubble-frontend-pwa/tree/master/%40hubblecommerce/hubble"
    },
    "keywords": [
        "hubble",
        "hubblecommerce",
        "PWA",
        "ecommerce",
        "shop",
        "magento",
        "shopware",
        "xtc",
        "frontend",
        "interface"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,scss,vue}": "prettier --write"
    }
}
